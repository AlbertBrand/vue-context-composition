{
  "name": "vue-context-composition",
  "version": "0.2.2",
  "description": "Vue.js context composition",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlbertBrand/vue-context-composition.git"
  },
  "bugs": {
    "url": "https://github.com/AlbertBrand/vue-context-composition/issues"
  },
  "homepage": "https://github.com/AlbertBrand/vue-context-composition#readme",
  "author": "Albert Brand <abrand@xebia.com>",
  "keywords": [
    "vuejs",
    "vue",
    "context",
    "composition",
    "typescript"
  ],
  "main": "./lib/node/index.js",
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "packemon build --addEngines",
    "validate": "packemon validate",
    "pack": "cross-env NODE_ENV=production packemon pack --addEngines --declaration=standard",
    "preversion": "git-is-clean && npm run test:lint && npm test",
    "version": "npm run pack && npm publish",
    "postversion": "cross-env VERSION=$(node -p \"require('./package.json').version\") && git add -A && git commit -m $VERSION && git tag v$VERSION -m '' && git push --follow-tags",
    "lint": "eslint --fix src/**/*.ts tests/**/*.ts && prettier .*.js package.json tsconfig.json tests/tsconfig.json --write --loglevel warn",
    "test": "jest",
    "test:lint": "eslint --max-warnings 0 src/**/*.ts tests/**/*.ts && prettier .*.js package.json tsconfig.json tests/tsconfig.json --check",
    "test:cov": "jest --coverage"
  },
  "peerDependencies": {
    "core-js": "^3.8.1",
    "vue": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/runtime-corejs3": "^7.12.5",
    "@types/jest": "^26.0.18",
    "@vue/test-utils": "^2.0.0-beta.10",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "core-js": "^3.8.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "flush-promises": "^1.0.2",
    "git-is-clean": "^3.0.1",
    "jest": "^26.6.3",
    "packemon": "^0.14.3",
    "prettier": "^2.2.1",
    "typescript": "^4.1.2",
    "vue": "^3.0.2",
    "vue-jest": "^5.0.0-alpha.6"
  },
  "packemon": {
    "platform": [
      "browser",
      "node"
    ]
  },
  "engines": {
    "node": ">=10.3.0",
    "npm": ">=6.1.0"
  },
  "module": "./esm/index.js",
  "browser": "./lib/browser/index.js",
  "files": [
    "dts/**/*.d.ts",
    "esm/**/*.{js,map}",
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "types": "./dts/index.d.ts"
}
